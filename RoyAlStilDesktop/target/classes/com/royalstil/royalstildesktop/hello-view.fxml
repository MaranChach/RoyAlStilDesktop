<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.royalstil.royalstildesktop.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText"/>
    <TableView fx:id="tableSex">
        <columns>
            <TableColumn fx:id="id_client" text="clid1"></TableColumn>
            <TableColumn fx:id="first_name_client" text="fn"></TableColumn>
            <TableColumn fx:id="second_name_client" text="sn"></TableColumn>
            <TableColumn fx:id="login_client" text="log"></TableColumn>
            <TableColumn fx:id="password_client" text="pass"></TableColumn>
            <TableColumn fx:id="phone_client" text="phone"></TableColumn>
            <TableColumn fx:id="email_client" text="email"></TableColumn>
            <TableColumn fx:id="birth_date_client" text="bd"></TableColumn>
        </columns>
    </TableView>
    <Button text="Hello!" onAction="#onHelloButtonClick"/>
</VBox>
